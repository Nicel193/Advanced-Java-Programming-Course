<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="405.0" prefWidth="582.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kukuiev.advjava.labthird.fourthtask.DictionaryController">
    <children>
        <VBox layoutX="270.0" layoutY="40.0" prefHeight="323.0" prefWidth="243.0">
            <children>
                <TableView fx:id="DictionaryTable" prefHeight="323.0" prefWidth="244.0">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
            </children></VBox>
      <VBox layoutX="63.0" layoutY="40.0" prefHeight="323.0" prefWidth="207.0">
         <children>
                <Button mnemonicParsing="false" onAction="#SearchWord" prefHeight="26.0" prefWidth="207.0" text="Пошук" />
            <TextField fx:id="findWordField" layoutX="10.0" layoutY="103.0" prefHeight="26.0" prefWidth="150.0" promptText="Знайти слово" />
                  <Button mnemonicParsing="false" onAction="#AddWords" prefHeight="31.0" prefWidth="262.0" text="Додати" />
                  <TextField fx:id="ua" prefHeight="26.0" prefWidth="150.0" promptText="Англійське слово" />
            <TextField fx:id="en" prefHeight="26.0" prefWidth="150.0" promptText="Українське слово" />
         </children>
      </VBox>
    </children>
</AnchorPane>
